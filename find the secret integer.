// find the secret integer.

import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
	    Scanner sc=new Scanner(System.in);
	    int tc=sc.nextInt();
	    while(tc-->0){
	    
	    int n=sc.nextInt();
	    int b[]=new int[n];
	    int allxor=0;
	    for(int i=0;i<n;i++){
	        b[i]=sc.nextInt();
	        allxor^=b[i];
	    }
	    int a[]=new int[n];
	    for(int i=0;i<n;i++){
	        a[i]=allxor^b[i];
	        	System.out.print(a[i]+" ");
	    }
	   
	   } 
	
	}
}
